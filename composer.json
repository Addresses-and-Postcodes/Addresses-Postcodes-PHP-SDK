{
    "name": "addresses-and-postcodes/lookup-php-sdk",
    "description": "Addresses and Postcodes - Lookup PHP SDK",
    "keywords": [
        "Postcodes",
        "Addresses"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Moussa Ball",
            "email": "moiseball20155@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "AddressesAndPostcodes\\Lookup\\PHP\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AddressesAndPostcodes\\Lookup\\PHP\\SDK\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}